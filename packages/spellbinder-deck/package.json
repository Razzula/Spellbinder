{
    "name": "spellbinder-deck",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "build:popup": "vite build --config vite.popup.config.ts",
        "build:inject-ws": "esbuild inject/ws-intercept.ts --bundle --target=es2020 --outfile=dist/inject/ws-intercept.js",
        "build:inject-react": "vite build --config vite.inject.config.ts",
        "build": "tsc && bun run build:popup && bun run build:inject-ws && bun run build:inject-react"
    },
    "dependencies": {
        "spellbinder": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "vite-plugin-svgr": "^4.3.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.66",
        "@types/react-dom": "^18.2.22",
        "@typescript-eslint/eslint-plugin": "^7.2.0",
        "@typescript-eslint/parser": "^7.2.0",
        "@vitejs/plugin-react": "^4.2.1",
        "eslint": "^8.57.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.6",
        "typescript": "^5.2.2",
        "vite": "^5.2.0"
    }
}